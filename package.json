{
  "name": "dannywraycouk",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "test": "node scripts/test.js",
    "serve": "serve -s build",
    "prod": "npm run build & npm run serve",
    "lint:js": "eslint \"src/**/*.js\"",
    "lint:js:fix": "eslint \"src/**/*.js\" --fix",
    "lint:css": "./node_modules/stylelint/bin/stylelint.js \"src/**/*.css\"",
    "lint:css:fix": "./node_modules/stylelint/bin/stylelint.js \"src/**/*.css\" --fix",
    "lint": "npm run lint:css && npm run lint:js",
    "major": "npm update && ./node_modules/npm-check-updates/bin/ncu -u && npm i && npm outdated",
    "importsort": "NODE_ENV=development BABEL_ENV=development node ./node_modules/import-sort-cli/lib/index.js 'src/**/*.js' --write"
  },
  "importSort": {
    ".js": {
      "parser": "babylon",
      "style": "react"
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "npm run --silent lint:js:fix",
      "npm run importsort",
      "git add ."
    ],
    "src/**/*.css": [
      "npm run --silent lint:css:fix",
      "git add ."
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "./node_modules/lint-staged/bin/lint-staged.js"
    }
  },
  "dependencies": {
    "@babel/core": "7.8.4",
    "@svgr/webpack": "5.1.0",
    "@typescript-eslint/eslint-plugin": "2.18.0",
    "@typescript-eslint/parser": "2.18.0",
    "babel-eslint": "10.0.3",
    "babel-jest": "^25.1.0",
    "babel-loader": "8.0.6",
    "babel-plugin-named-asset-import": "^0.3.5",
    "babel-preset-react-app": "^9.1.0",
    "camelcase": "^5.3.1",
    "case-sensitive-paths-webpack-plugin": "2.3.0",
    "css-loader": "3.4.2",
    "csslint": "^1.0.5",
    "dotenv": "8.2.0",
    "dotenv-expand": "5.1.0",
    "eslint-config-react-app": "^5.1.0",
    "eslint-loader": "3.0.3",
    "eslint-plugin-flowtype": "4.6.0",
    "file-loader": "5.0.2",
    "fs-extra": "8.1.0",
    "hoist-non-react-statics": "^3.3.2",
    "html-webpack-plugin": "4.0.0-beta.5",
    "http-status-codes": "^1.4.0",
    "identity-obj-proxy": "3.0.0",
    "is-hexadecimal": "^1.0.4",
    "is-wsl": "^2.1.1",
    "jest": "25.1.0",
    "jest-environment-jsdom-fourteen": "1.0.1",
    "jest-resolve": "25.1.0",
    "jest-watch-typeahead": "0.4.2",
    "lodash.get": "^4.4.2",
    "mini-css-extract-plugin": "0.9.0",
    "optimize-css-assets-webpack-plugin": "5.0.3",
    "pnp-webpack-plugin": "1.6.0",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-normalize": "8.0.1",
    "postcss-preset-env": "6.7.0",
    "postcss-safe-parser": "4.0.1",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-app-polyfill": "^1.0.5",
    "react-async": "10.0.0",
    "react-dev-utils": "^10.0.0",
    "react-dom": "^16.12.0",
    "react-helmet": "5.2.1",
    "react-redux": "7.1.3",
    "react-redux-form": "1.16.14",
    "react-router-dom": "5.1.2",
    "react-socks": "^2.1.0",
    "rehype-katex": "^2.0.0",
    "rehype-stringify": "^6.0.1",
    "remark-math": "^2.0.0",
    "remark-parse": "^7.0.2",
    "remark-rehype": "^5.0.0",
    "resolve": "1.15.0",
    "resolve-url-loader": "3.1.1",
    "sass-loader": "8.0.2",
    "semver": "7.1.1",
    "style-loader": "1.1.3",
    "stylelint": "^13.0.0",
    "terser-webpack-plugin": "2.3.4",
    "ts-pnp": "1.1.5",
    "unified": "^8.4.2",
    "url-loader": "3.0.0",
    "webpack": "4.41.5",
    "webpack-dev-server": "3.10.1",
    "webpack-manifest-plugin": "2.2.0",
    "workbox-webpack-plugin": "5.0.0"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-standard": "^14.1.0",
    "eslint-plugin-import": "^2.20.0",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.18.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "eslint-plugin-standard": "^4.0.1",
    "husky": "^4.2.1",
    "import-sort": "6.0.0",
    "import-sort-cli": "6.0.0",
    "import-sort-parser-babylon": "^6.0.0",
    "import-sort-style-react": "^5.1.0",
    "lint-staged": "^10.0.6",
    "npm-check-updates": "^4.0.1"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jest-environment-jsdom-fourteen",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  }
}
